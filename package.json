{
  "name": "openapi-platform",
  "description": "Manage platform users, access keys, system authentication, permissions, etc.",
  "version": "1.15.7",
  "main": "dist/src/index.js",
  "repository": "https://github.com/orangelab-kr/openapi-platform.git",
  "author": "bakjonghun <bakjonghun@hikick.kr>",
  "license": "MIT",
  "scripts": {
    "serve": "yarn serve:dev",
    "serve:dev": "sls offline --stage dev",
    "serve:prod": "sls offline --stage prod",
    "serve:stage": "sls offline --stage stage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "deploy": "yarn deploy:stage",
    "deploy:prod": "sls deploy --stage prod",
    "deploy:stage": "sls deploy --stage stage",
    "purge": "yarn purge:stage",
    "purge:prod": "sls remove --stage prod",
    "purge:stage": "sls remove --stage stage",
    "premigrate": "prisma format",
    "migrate": "prisma db push",
    "postmigrate": "node prisma/setup.js"
  },
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "@sentry/node": "^6.18.2",
    "@sentry/tracing": "^6.18.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "i18n": "^0.14.2",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "serverless-http": "^2.7.0",
    "winston": "^3.6.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/copy-webpack-plugin": "^10.1.0",
    "@types/cors": "^2.8.12",
    "@types/eslint": "^8.4.1",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/i18n": "^0.13.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.21",
    "@types/prettier": "^2.4.4",
    "@types/serverless": "^3.0.1",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "copy-webpack-plugin": "^10.2.4",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.0",
    "prisma": "^3.11.0",
    "serverless": "^3.7.9",
    "serverless-doppler": "^1.0.2",
    "serverless-offline": "^8.5.0",
    "serverless-webpack": "5.6.1",
    "serverless-webpack-prisma": "^1.1.0",
    "ts-loader": "^9.2.8",
    "typescript": "4.6.2",
    "webpack": "5.70.0",
    "webpack-node-externals": "^3.0.0"
  }
